/* pages/gallery/component/multiphoto/index.wxss */
page {
    width: 100%;
    height: 100%;
    overflow: hidden;
}

image {
    display: inline-block;
}

.mode-box-size {
    position: relative;
    flex-shrink:0;
}

.mode-cover {
    width: 100%;
    display: inline-block;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 99;
}

.img-area {
    position: absolute;
    overflow: hidden;
    display: flex;
    justify-content: center;
    align-items: center;
}

.addimg {
    width: 50%;
    height: 50%;
}

.border-cover{
    position: absolute;
    overflow: hidden;
    z-index: 900;
    box-sizing:border-box;
}

.img-area-cover {
    position: absolute;
    overflow: hidden;
    z-index: 999;
}

/**拍照功能**/
.component-choosePhoto {
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 99999;
}

.component-choosePhoto-container {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: flex-end;
    animation: alp 0.5s ease;
    background: rgba(0, 0, 0, 0.5);
}

.component-choosePhoto-content {
    width: 100%;
    background: #efeff5;
    animation: chooseBottom 0.3s ease;
    position: relative;
}

.component-choosePhotoItem {
    width: 100%;
    height: 90rpx;
    border-bottom: 1rpx solid #efeff5;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #fff;
}

.component-choosePhotoItem-text {
    color: #333;
    font-size: 34rpx;
}

.canvas-style {
    position: fixed;
    z-index: -999;
    left: -99999px;
    right: -999999px;
}

.img-edit-box{
    width:100%;
    height:100%;
    position:absolute;
    top:0;
    left:0;
    display:flex;
    justify-content:center;
    align-items:center;
    background:rgba(0,0,0,0.3);
}

.icon-box{
    display:flex;
    height:100%;
    flex:1;
    justify-content:center;
    align-items:center;
    position:relative;
    z-index:1999;
}

.img-icon-style{
    font-size:80rpx;
    color:#fae100;
}

.line{
    width:2rpx;
    height:100%;
    background:#fae100;
}

.iconfont {
    font-family: "iconfont" !important;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-swap:before {
    content: "\e7f2";
}

.icon-close:before {
    content: "\e7fc";
}

@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}
