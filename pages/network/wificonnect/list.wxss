/* pages/network/wificonnect/list.wxss */
page {
  height: 100%;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

view {
  box-sizing: border-box;
}

.wifilist-box {
  width: 100%;
  padding-bottom: 160rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.wifilist-tips {
  font-size: 36rpx;
  color: #333;
  margin: 40rpx 0;
  width: 100%;
  text-align: center;
}

.wifilist {
  padding: 30rpx 20rpx;
  width: 100%;
  display: flex;
  align-items: center;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #333;
  justify-content: space-between;
}

.wifi-name {
  width:400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
    url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
    url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 50rpx;
  font-weight: 600;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-wifi:before {
  content: "\e6a4";
}

.nowifi-tips {
  font-size: 36rpx;
  color: #333;
  margin-top: 250rpx;
  width: 100%;
  text-align: center;
}
