.handle-bottom,
.page-over,
.bottom-bar,
.bar-btn,
.bar-btns {
  display: flex;
}

.handle-bottom,
.bar-btn {
  flex-direction: column;
}

.page-over,
.bottom-bar,
.bar-btn {
  align-items: center;
}

.swiper-box {
  height: 80%;
  overflow-y: scroll;
}

.box-item image {
  width: 100%;
}

.handle-bottom {
  height: 20%;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  justify-content: flex-end;
}

.page-over {
  justify-content: center;
  color: #999;
  font-size: 24rpx;
}

.page-over image {
  width: 50rpx;
  height: 50rpx;
  padding: 0 12rpx;
}

.bottom-bar {
  height: 100rpx;
  padding: 0 24rpx;
  margin-top: 32rpx;
  border-top: 2rpx solid #E7E7E7;
}

.bar-btn {
  width: 18%;
  justify-content: center;
  font-size: 24rpx;
  color: #666;
}

.bar-btn image {
  width: 36rpx;
  height: 36rpx;
}

.bar-btns {
  width: 64%;
}

.bar-btns>view {
  width: 50%;
  height: 76rpx;
  line-height: 76rpx;
  text-align: center;
  font-size: 32rpx;
  color: #333;
}

.btns-consumable {
  background: #FFA200;
  border-top-left-radius: 38rpx;
  border-bottom-left-radius: 38rpx;
}

.btns-print {
  background: #FFDC5E;
  border-top-right-radius: 38rpx;
  border-bottom-right-radius: 38rpx;
}