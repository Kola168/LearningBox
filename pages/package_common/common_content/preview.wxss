/* pages/package_common/common_content/preview.wxss */
page {
    position: relative;
    width: 100%;
    font-size: 36rpx;
  }

  .wrap-fun {
    position: relative;
    width: 100%;
    height: calc(100% - 100rpx);
    overflow-y: scroll;
  }
  
  .wrap-fun {
    height: calc(100% - 100rpx - 130rpx);
    padding-bottom: 130rpx;
    position: relative;
  }

  .wrap-fun>image {
    width: 750rpx;
  }

  .swiper-box {
    width: 100%;
    height: 100%;
  }
  .watermark {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 88%;
  }

  .study-none {
    width: 100%;
    text-align: center;
  }
  
  .study-none image {
    width: 50%;
    margin-top: 200rpx;
  }

  .turn {
    display: flex;
    position: fixed;
    align-items: center;
    left: 0;
    bottom: 100rpx;
    padding: 40rpx 0;
    width: 100%;
    box-sizing: border-box;
    background-color: #fff;
  }
  
  .turn-image {
    display: -webkit-flex;
    margin-right: 306rpx;
  }
  
  .turn-content {
    display: flex;
    width: 100%;
    box-sizing: border-box;
    padding-left: 24rpx;
    font-size: 24rpx;
  }
  
  .turn-image-item {
    width: 50rpx;
    vertical-align: top;
  }
  
  .turn-image-left {
    margin-right: 36rpx;
  }

  .bottom {
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: #fff;
    box-sizing: border-box;
    border-top: 3rpx solid #cecece;
    position: fixed;
    bottom: 0;
    padding-left: 50rpx;
    padding-right: 24rpx;
    box-sizing: border-box;
  }
  
  .bottom-image {
    width: 30rpx;
    height: 30rpx;
  }
  
  .bottom .left {
    display: flex;
    border-radius: 20rpx;
  
  }
  
  .bottom .right {
    display: flex;
    width: 456rpx;
    border-radius: 60rpx;
    overflow: hidden;
  }

  .collect {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-right: 60rpx;
  }
  
  .collect view {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
  }

  .font-content {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #666666;
    margin-top: 4rpx;
  }

  .share {
    display: flex;
    align-items: center;
    justify-content: center;
    margin-right: 40rpx;
  }
  
  .share button{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    line-height: 30rpx;
  }

  .share-button {
    display: flex;
    align-items: center;
    margin-left: 0;
    margin-right: 0;
  }

  .bottom .right-first {
    position: relative;
    width: 228rpx;
    height: 74rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #ffa200 !important;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #FFFFFF;
    letter-spacing: -1.22rpx;
    text-align: center;
  }

  .bottom .print-button {
  font-family: PingFangSC-Medium;
  width: 228rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background: #FF5022;
  height: 74rpx;
  font-size: 30rpx;
  color: #fff;
}
  
  
  