/* pages/package_common/common_content/preview.wxss */
page {
    position: relative;
    width: 100%;
    height: 100%;
    font-size: 36rpx;
}

.wrap-fun {
    position: relative;
    width: 100%;
    /* top:176rpx; */
    height: calc(100% - 100rpx);
    overflow-y: scroll;
}

.wrap-fun {
    height: calc(100% - 100rpx - 130rpx);
    padding-bottom: 130rpx;
    position: relative;
}

.wrap-fun>image {
    width: 750rpx;
}

.swiper-box {
    width: 100%;
    height: 100%;
}

.watermark {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 88%;
}

.study-none {
    width: 100%;
    text-align: center;
}

.study-none image {
    width: 50%;
    margin-top: 200rpx;
}

.turn {
    display: flex;
    position: fixed;
    align-items: center;
    left: 0;
    bottom: 100rpx;
    padding: 40rpx 0;
    width: 100%;
    box-sizing: border-box;
    background-color: #fff;
}

.turn-image {
    display: -webkit-flex;
    margin-right: 306rpx;
}

.turn-content {
    display: flex;
    width: 100%;
    box-sizing: border-box;
    padding-left: 24rpx;
    font-size: 24rpx;
}

.turn-image-item {
    width: 50rpx;
    vertical-align: top;
}

.turn-image-left {
    margin-right: 36rpx;
}

.bottom {
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: #fff;
    box-sizing: border-box;
    border-top: 3rpx solid #cecece;
    position: fixed;
    bottom: 0;
    padding-left: 50rpx;
    padding-right: 24rpx;
    box-sizing: border-box;
}

.bottom-image {
    width: 30rpx;
    height: 30rpx;
}

.bottom .left {
    display: flex;
    border-radius: 20rpx;

}

.bottom .right {
    display: flex;
    width: 456rpx;
    border-radius: 60rpx;
    overflow: hidden;
}

.collect {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-right: 60rpx;
}

.collect view {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.font-content {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #666666;
    margin-top: 4rpx;
}

.share {
    display: flex;
    align-items: center;
    justify-content: center;
    margin-right: 40rpx;
}

.share button {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    line-height: 30rpx;
}

.share-button {
    display: flex;
    align-items: center;
    margin-left: 0;
    margin-right: 0;
}

.bottom .right-first {
    position: relative;
    width: 228rpx;
    height: 74rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #ffa200 !important;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #FFFFFF;
    letter-spacing: -1.22rpx;
    text-align: center;
}

.bottom .print-button {
    position: relative;
    font-family: PingFangSC-Medium;
    width: 228rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background: #FF5022;
    height: 74rpx;
    font-size: 30rpx;
    color: #fff;
}

.document_title {
    /* width: 100%; */
    display: flex;
    align-items: center;
    padding: 24rpx;
    border-bottom: 20rpx solid #f5f5f5;
}

.document_title>view {
    color: #333;
    font-size: 35rpx;
    font-weight: 500;
    margin-right: 20rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    font-weight: bold;
}

.setting_item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    line-height: 110rpx;
    border-bottom: 1rpx solid #cecece;
}

.item_label {
    font-size: 32rpx;
}

.item_main {
    display: flex;
    flex-direction: row;
    align-items: center;
    color: #999;
}

.print_num {
    width: 30rpx;
    height: 30rpx;
}

.document_print_num {
    min-width: 40rpx;
    text-align: center;
    margin: 0 10rpx;
    font-size: 30rpx;
}

.page_text {
    font-size: 26rpx;
}

.page_input {
    width: 80rpx;
    height: 45rpx;
    margin: 0 15rpx;
    box-shadow: 0 2rpx 5rpx 2rpx #efefef inset;
    color: #999;
    font-size: 24rpx;
    text-align: center;
    border-radius: 5rpx;
    overflow: hidden;
}

.color_check_box {
    display: flex;
    align-items: center;
    margin-left: 10rpx;
}

.color_check {
    width: 42rpx;
}

.color_type {
    font-size: 28rpx;
    margin: 0 16rpx;
}

.zoom_view {
    padding: 36rpx 30rpx 100rpx;
}

.view_title {
    display: flex;
    justify-content: space-between;
    font-size: 32rpx;
    color: #333;
}

.view_title>view:last-child {
    font-size: 20rpx;
    color: #000;
    height: 44rpx;
    line-height: 44rpx;
    border-radius: 22rpx;
    padding: 0 20rpx;
    background: #ffe27a;
}

.view_content {
    padding: 30rpx 46rpx;
    display: flex;
    flex-wrap: wrap;
}

.content_item {
    width: 26%;
    display: flex;
    flex-direction: column;
    text-align: center;
    color: #666;
    font-size: 28rpx;
    margin-bottom: 32rpx;
}

.content_item:nth-child(3n-1) {
    margin: 0 11%;
}

.content_item image:first-child {
    width: 100%;
}

.item_img {
    position: relative;
    margin-bottom: 4rpx;
}

.item_check_active {
    border: 4rpx solid #ffe27a;
}

.item_check {
    position: absolute;
    right: 8rpx;
    bottom: 32rpx;
    width: 36rpx;
}

.conf_box-left {
    color: #454545;
    width: 50%;
    background: #fff;
    line-height: 98rpx;
    text-align: center;
    position: fixed;
    left: 0;
    bottom: 0;
    border-top: 3rpx solid #cecece;
}

.conf_box-right {
    position: relative;
    color: #454545;
    width: 50%;
    background: #ffe27a;
    line-height: 98rpx;
    text-align: center;
    position: fixed;
    right: 0;
    bottom: 0;
    border-top: 3rpx solid #cecece;
}

.consumables-icon {
    width: 228rpx;
    height: 74rpx !important;
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
}

.ios-tip {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.5);
    z-index: 10000;
    display: flex;
    justify-content: center;
    align-items: center;
}

.ios-tip-content {
    background: #fff;
    width: 80%;
    border-radius: 16rpx;
    overflow: hidden;
}

.ios-tip-content .text-1 {
    font-family: PingFangSC-Medium;
    font-size: 36rpx;
    color: #454545;
    letter-spacing: -0.98rpx;
    text-align: center;
    padding: 80rpx 0 50rpx;
}

.ios-tip-content .text-2 {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #333333;
    letter-spacing: -0.16px;
    padding: 0 36rpx;
}

.ios-tip-content .text-3 {
    font-family: PingFangSC-Medium;
    font-size: 36rpx;
    color: #454545;
    letter-spacing: -0.98px;
    text-align: center;
    background: #FFE27A;
    height: 88rpx;
    line-height: 88rpx;
    margin-top: 72rpx;
}

.ios-tip-modal {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.50);
    display: flex;
    justify-content: center;
    align-items: center;
}

.ios-tip-text {
    margin: 0 40rpx;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #666666;
    letter-spacing: 0.62px;
}

.doc_setting {
    box-sizing: border-box;
    padding: 0 30rpx;
}

.bottom-button-box {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    z-index: 1000
}