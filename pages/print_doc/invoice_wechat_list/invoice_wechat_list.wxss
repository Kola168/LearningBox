Page {
  width: 100%;
  height: 100%;
}

.list-container {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 150rpx;
}

.doc-item-container {
  position: relative;
  display: flex;
  width: 100%;
}

.close {
  width: 28rpx;
}

.doc-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 100%;
  box-sizing: border-box;
  font-size: 28rpx;
  border-bottom: 20rpx solid #f5f5f5;
}

.doc-item-name {
  display: flex;
  align-items: center;
  padding: 24rpx;
}

.title {
  width: 96%;
  overflow: hidden;
  max-length: 500rpx;
  text-overflow: ellipsis;
  font-size: 32rpx;
  color: #000;
}

.preview-notify {
  display: flex;
  flex-direction: row;
}

.excel-notify {
  width: 270rpx;
  max-width: 270rpx;
  word-wrap: break-word;
  font-size: 20rpx;
  margin-left: 20rpx;
  color: #fe8d77;
}

.line {
  width: 100%;
  background: #cecece;
  height: 2rpx;
}

.item-setting {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  padding: 30rpx 24rpx;
}

.print-setting {
  font-size: 30rpx;
  color: #2086ee;
}

.item-preview {
  display: flex;
  flex-direction: row;
}

.item-preview .icon-see {
  width: 40rpx;
}

.item-preview .text-preview {
  width: 160rpx;
  height: 60rpx;
  border-radius: 30rpx;
  background: #FFDC5E;
  font-size: 28rpx;
  color: #333;
  line-height: 60rpx;
  text-align: center;
}

.bottom {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background: #fff;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  z-index: 888;
  border-top: 2rpx solid #cecece;
}

.bottom-tip-medium {
  display: flex;
  position: fixed;
  left: 0;
  bottom: 130rpx;
  z-index: 888;
  width: 100%;
  height: 30rpx;
  align-items: center;
  justify-content: center;
}

.bottom-tip-medium-text {
  font-size: 24rpx;
  color: #999;
}

.bottom-left {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50%;
  height: 100%;
  font-size: 36rpx;
  border-right: 2rpx solid #fff;
  box-sizing: border-box;
  color: #2086ee;
}

.bottom-right {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 50%;
  font-size: 32rpx;
  color: #666;
}

.bottom-count {
  position: absolute;
  left: 0;
  bottom: 100rpx;
  width: 100%;
  color: #fff;
  font-size: 24rpx;
  padding: 6rpx 0;
  text-align: center;
  background: rgba(0, 0, 0, 0.6);
}

.bottom form {
  width: 100%;
}

.bottom button {
  width: 100%;
  padding-left: 60rpx;
  padding-right: 60rpx;
  line-height: 100rpx;
  border-radius: 0;
  background: #FFDC5E;
  color: #000;
}

.button-gray {
  background: #f5f5f5!important;
  border-left: 2rpx solid #fff;
}

.progress {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1000;
  display: flex;
  flex-direction: row-reverse;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;  
  padding-bottom: 60rpx;
  box-sizing: border-box;
  background: rgba(0, 0, 0, 0.2);
}

.progress-content {
  position: relative;
  padding: 100rpx 80rpx;
  background: #fff;
  border-radius: 10rpx;
  color: #676767;
  font-size: 32rpx;
}

.cancel-upload {
  position: absolute;
  bottom: 30rpx;
  right: 30rpx;
  font-size: 28rpx;
  line-height: 50rpx;
  padding: 0 16rpx;
  border-radius: 4rpx;
  color: #999;
}

.progress-count {
  margin-bottom: 30rpx;
  text-align: center;
  font-size: 36rpx;
  color: #FFDC5E;
}

.progress-bar {
  margin-top: 30rpx;
  border-radius: 12rpx;
}

.progress-loading {
  display: block;
  margin: 30rpx auto 0 auto;
  width: 80rpx;
  height: 80rpx;
}

.showInterceptModal {
  margin: 0 40rpx;
  padding: 20rpx;
  font-size: 26rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  color: #fff;
  background: rgba(0, 0, 0, 0.75);
}