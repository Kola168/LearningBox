/* pages/package_preschool/exercise_day/finish/index.wxss */

.finish-main {
  width: 100%;
  min-height: 100vh;
  background-color: #fff;
}

.finish-main .banner {
  width: calc( 100% - 48rpx );
  margin: 24rpx 24rpx 0;
}

.finish-name {
  padding: 60rpx 0;
  text-align: center;
  font-size: 32rpx;
  color: #454545;
}

.btn {
  margin: 0 auto 80rpx;
  width: 600rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 50rpx;
  text-align: center;
  font-size: 36rpx;
  color: #333333;
  background: #FFDC5E;
}

.consumable {
  width: 100%;
}