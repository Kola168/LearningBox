/* pages/package_feature/print_sticker/imglist.wxss */
page {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.top-check-box {
  width: 410rpx;
  height: 66rpx;
  margin-top: 26rpx;
  display: flex;
  justify-content: space-between;
}

.check-item {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  color: #666666;
}

.check-item-checked {
  color: #FFA200;
}

.line {
  width: 28rpx;
  height: 4rpx;
  background: #FFA200;
}

.tips-box {
  font-size: 24rpx;
  color: #666666;
  display: flex;
  align-items: center;
  margin-top: 28rpx;
}

.tips-icon {
  width: 36rpx;
  margin-right: 10rpx;
}

.flex-img-box {
  display: flex;
  justify-content: center;
  align-items: center;
  align-content: center;
  flex-wrap: wrap;
  flex: 1;
}

.img-box-item {
  display: flex;
  margin: 20rpx;
}

.img-item {
  width: 100%;
  height: 100%;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.img-style {
  width: 100%;
  height: 100%;
}

.img-edit-box {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  display: flex;
  font-size: 24rpx;
  color: #333333;
  align-items: center;
  line-height: 40rpx;
  background: #FFDC5E;
}

.img-edit-item {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}

.img-edit-line {
  width: 2rpx;
  height: 22rpx;
  background: #454545;
}

.no-img-bg {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}

.img-add-icon {
  width: 80rpx;
}

.bottom-but{
  width:100%;
}
