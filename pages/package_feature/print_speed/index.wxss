.size-item {
  width: 100rpx;
  height: 60rpx;
  font-size: 30rpx;
  color: #666;
  border: 1px solid #ededed;
  border-radius: 50rpx;
  text-align: center;
  line-height: 60rpx;
  margin-right: 40rpx;
}

.size-item-active {
  width: 100rpx;
  height: 60rpx;
  font-size: 30rpx;
  color: #666;
  border: 1px solid #ededed;
  border-radius: 50rpx;
  text-align: center;
  line-height: 60rpx;
  margin-right: 40rpx;
  background: #ffe27a;
}

.bottom-save {
  background: #ffe27a;
  width: 100%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 36rpx;
  color: #454545;
  position: fixed;
  bottom: 0;
  left: 0;
}

.modal-wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 9999;
}

.confirm-title {
  color: #333;
  font-size: 36rpx;
  padding: 0 46rpx;
}

.button-bottom {
  width: 100%;
  display: flex;
  flex-direction: row;
  margin-top: 88rpx;
  border-top: 2rpx solid #cecece;
}

.button-confirm {
  width: 50%;
  text-align: center;
  font-size: 36rpx;
  color: #000;
  height: 80rpx;
  line-height: 80rpx;
  background: #ffe27a;
  border-top: 2rpx solid #ffe27a;
  border-radius: 0 0 20rpx 0;
}

.button-cancel {
  width: 50%;
  text-align: center;
  font-size: 36rpx;
  color: #666;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 0 0 0 20rpx;
}

.confirm-modal1 {
  width: 620rpx;
  background: #fff;
  color: #333;
  border-radius: 20rpx;
  padding-top: 80rpx;
  text-align: center;
}

.ql-active {
  color: #06c;
}

.iconfont {
  display: inline-block;
  padding: 5px;
  width: 24px;
  height: 24px;
  cursor: pointer;
  font-size: 20px;
}

