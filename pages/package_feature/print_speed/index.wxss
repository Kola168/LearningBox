.top {
  width: 100%;
  height: 100%;
}

.wrap,
.wrap-fun {
  position: relative;
  width: 100%;
  height: calc(100% - 100rpx);
  overflow-y: scroll;
}

.wrap-fun {
  height: calc(100% - 100rpx - 130rpx);
  padding-bottom: 130rpx;
  position: relative;
}

.wrap>image,
.wrap-fun>image {
  width: 750rpx;
}

.turn {
  display: flex;
  position: fixed;
  align-items: center;
  left: 0;
  bottom: 100rpx;
  padding: 40rpx 0;
  width: 100%;
  box-sizing: border-box;
  background-color: #fff;
}

.turn-image {
  display: -webkit-flex;
  margin-right: 306rpx;
}

.turn-content {
  display: flex;
  width: 100%;
  box-sizing: border-box;
  padding-left: 24rpx;
  font-size: 24rpx;
}

.turn-image-item {
  width: 50rpx;
  vertical-align: top;
}

.turn-image-left {
  margin-right: 36rpx;
}

.print-preview {
  position: absolute;
  left: 30%;
  bottom: 110rpx;
  width: 40%;
}

.bottom {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  box-sizing: border-box;
  border-top: 3rpx solid #cecece;
  position: fixed;
  bottom: 0;
}

.bottom-image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}

.bottom .print-button,
.bottom .pay-button {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background: #ffe27a;
  height: 100rpx;
  width: 300rpx;
  font-size: 36rpx;
  color: #333;
}

.swiper-box {
  width: 100%;
  height: 100%;
}

.watermark {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 88%;
}

.share {
  display: flex;
  flex-grow: 1;
  align-items: center;
}

.share-button {
  display: flex;
  align-items: center;
  margin-left: 0;
  margin-right: 0;
}

.collect {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-grow: 2;
}

.font-content {
  font-size: 32rpx;
  color: #666;
}

.price {
  font-size: 22rpx;
  margin: 0 17rpx 0 11rpx;
}

