}

.price-sale {
  font-size: 22rpx;
  color: #999;
  text-decoration: line-through;
}

.doc_setting {
  box-sizing: border-box;
  padding: 0 30rpx;
}

.document_title {
  /* width: 100%; */
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 24rpx;
  border-bottom: 20rpx solid #f5f5f5;
}

.title-left {
  color: #333;
  font-size: 35rpx;
  font-weight: 500;
  margin-right: 20rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-weight: bold;
}

.title-right {
  color: #333;
  font-size: 26rpx;
  width: 100rpx;
  height: 50rpx;
  background: #ffe27a;
  text-align: center;
  border-radius: 25rpx;
  line-height: 50rpx;
  /* margin-right: 20rpx; */
  /* text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap; */
}

.setting_item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 110rpx;
  border-bottom: 1rpx solid #cecece;
}

.item_label {
  font-size: 32rpx;
}

.item_main {
  display: flex;
  flex-direction: row;
  align-items: center;
  color: #999;
}

.print_num {
  width: 30rpx;
  height: 30rpx;
}

.document_print_num {
  min-width: 40rpx;
  text-align: center;
  margin: 0 10rpx;
  font-size: 30rpx;
}

.page_text {
  font-size: 26rpx;
}

.page_input {
  width: 80rpx;
  height: 45rpx;
  margin: 0 15rpx;
  box-shadow: 0 2rpx 5rpx 2rpx #efefef inset;
  color: #999;
  font-size: 24rpx;
  text-align: center;
  border-radius: 5rpx;
  overflow: hidden;
}

.color_check_box {
  display: flex;
  align-items: center;
  margin-left: 10rpx;
}

.color_check {
  width: 42rpx;
}

.color_type {
  font-size: 28rpx;
  margin: 0 16rpx;
}

.zoom_view {
  padding: 36rpx 30rpx 100rpx;
}

.view_title {
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
  color: #333;
}

.view_title>view:last-child {
  font-size: 20rpx;
  color: #000;
  height: 44rpx;
  line-height: 44rpx;
  border-radius: 22rpx;
  padding: 0 20rpx;
  background: #ffe27a;
}

.view_content {
  padding: 30rpx 46rpx;
  display: flex;
  flex-wrap: wrap;
}

.content_item {
  width: 26%;
  display: flex;
  flex-direction: column;
  text-align: center;
  color: #666;
  font-size: 28rpx;
  margin-bottom: 32rpx;
}

.content_item:nth-child(3n-1) {
  margin: 0 11%;
}

.content_item image:first-child {
  width: 100%;
}

.item_img {
  position: relative;
  margin-bottom: 4rpx;
}

.item_check_active {
  border: 4rpx solid #ffe27a;
}

.item_check {
  position: absolute;
  right: 8rpx;
  bottom: 32rpx;
  width: 36rpx;
}

.conf_box-left {
  color: #454545;
  width: 50%;
  background: #fff;
  line-height: 98rpx;
  text-align: center;
  position: fixed;
  left: 0;
  bottom: 0;
  border-top: 3rpx solid #cecece;
}

.conf_box-right {
  position: relative;
  color: #454545;
  width: 50%;
  background: #ffe27a;
  line-height: 98rpx;
  text-align: center;
  position: fixed;
  right: 0;
  bottom: 0;
  border-top: 3rpx solid #cecece;
}

.conf_box-left.butHigh,
.conf_box-right.butHigh{
  bottom: 40rpx;
}

.consumables-icon {
  width: 100rpx;
  height: 104rpx;
  position: fixed;
  bottom: 160rpx;
  right: 30rpx;
  z-index: 10000;
}

.bottom-button-box {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: transparent;
  z-index: 1000
}
