/* pages/package_feature/print_ calendar/typelist.wxss */
page {
  width: 100%;
  padding: 44rpx 24rpx;
  box-sizing: border-box;
}

.flex-box {
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.item-box {
  width: 336rpx;
  height: 344rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-shadow: 0 2px 8px 0 rgba(204, 204, 204, 0.40);
  background: #fff;
  border-radius:10rpx;
  overflow:hidden;
}

.item-img {
  width: 100%;
}

.item-text {
  flex: 1;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  color: #333333;
}
