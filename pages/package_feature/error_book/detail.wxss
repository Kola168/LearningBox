page {
  width: 100%;
  /* height: 100%; */
  box-sizing: border-box;
  padding-bottom: 146rpx;
}

.top {
  width: 100%;
  height: 88rpx;
  display: flex;
  justify-content: space-between;
  /* padding: 0 22rpx; */
  box-sizing: border-box;
}

.top1 {
  position: fixed;
  width: 100%;
  height: 88rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 22rpx;
  box-sizing: border-box;
  background: #fff;
  z-index: 1000;
}

.top_icon {
  font-size: 30rpx;
  display: flex;
  align-items: center;
}

.error_icon {
  width: 34rpx;
  height: 34rpx;
  margin-right: 12rpx;
}

.border2 {
  height: 20rpx;
  width: 100%;
  background: #f7f7f7;
  margin-bottom: 14rpx;
}

.date {
  color: #666;
  font-size: 24rpx;
  text-align: center;
  border-bottom: 2rpx solid #e3e3e3;
  padding-bottom: 12rpx;
  margin-bottom: 20rpx;
}

.date1 {
  margin-right: 20rpx;
}

.des {
  display: flex;
  align-items: center;
  margin-bottom: 24rpx;
}

.left-icon {
  display: flex;
}

.error_icon1 {
  width: 34rpx;
  margin-left: 24rpx;
}

.describe {
  background: #eaeaea;
  border-radius: 21px;
  font-size: 12px;
  color: #666666;
  margin-left: 22rpx;
  padding: 10rpx 26rpx;
}

.img {
  position: relative;
  width: 702rpx;
  height: 344rpx;
  margin: 0 24rpx 12rpx 24rpx;
  vertical-align: top;
}

.anwser {
  position: absolute;
  top: 0;
  right: 0;
  background: #53BB29;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  padding:6rpx 12rpx;
  border-bottom-left-radius: 8rpx;
}

.all {
  color: #999;
  font-size: 32rpx;
  text-align: center;
  margin-top: 24rpx;
}

.num {
  position: fixed;
  bottom: 100rpx;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background: #fff;
}

.center {
  margin-top: 350rpx;
}

.btn {
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  background: #fff;
  border-top: 2rpx solid #f5f5f5;
}

.btn>view {
  color: #999;
  font-size: 32rpx;
  width: 60%;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
  justify-content: space-between;
}

.btn button {
  width: 40%;
  height: 100rpx;
  line-height: 100rpx;
  background: #ffe27a;
  padding:0
}

.modal-wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 9999;
}

.confirm-modal {
  width: 90%;
  background: #fff;
  color: #333;
  font-size: 36rpx;
  border-radius: 20rpx;
  padding-top: 40rpx;
  text-align: center;
}

.confirm-modal image {
  width: 80%;
  margin-top: 40rpx;
  margin-bottom: 60rpx;
}

.print {
  background: #ffe27a;
  color: #454545;
  font-size: 36rpx;
  border-radius: 0 0 20rpx 20rpx;
}

.header1 {
  background-color: #f5f5f5;
}

.header2 {
  background-color: #ffffff;
}

.header {
  width: 100%;
  height: 80rpx;
  border-bottom: 1rpx solid #d6d6d6;
}

.header .header_con {
  width: 700rpx;
  height: 80rpx;
  margin: 0 auto;
  color: #333;
  font-size: 30rpx;
}

.header .header_con image {
  width: 44rpx;
  height: 44rpx;
}

.header .header_con .right {
  color: #ff4965;
}

.bottom {
  width: 100%;
  height: 100rpx;
  border-top: 1rpx solid #d6d6d6;
  background-color: #fff;
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
}

.bottom .left {
  margin-left: 25rpx;
}

.bottom .left .select_none {
  width: 60rpx;
  height: 60rpx;
  border: 1rpx solid #d6d6d6;
  border-radius: 50%;
}

.bottom .right .all {
  color: #ff830f;
  font-size: 26rpx;
}

.bottom .right .gray {
  color: #666666;
  font-size: 22rpx;
}

.bottom .right .sure {
  margin-left: 22rpx;
  width: 220rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: #ff4965;
  color: #fff;
}

/*列表*/
.onlinechoose {
  width: 750rpx;
  margin-top: 15rpx;
  background-color: #ffffff;
  font-size: 32rpx;
}

.onlinechoose .hotel_list {
  width: 750rpx;
  height: 260rpx;
  border-bottom: 1rpx solid #d6d6d6;
  background-color: #ffffff;
}

.onlinechoose .hotel_list .icon_none {
  width: 60rpx;
  height: 60rpx;
  border: 1rpx solid #d6d6d6;
  border-radius: 50%;
}

.onlinechoose .hotel_list .list_cons {
  width: 700rpx;
  height: 200rpx;
}

.onlinechoose .list_cons .lefts {
  width: 80rpx;
}

.onlinechoose .list_cons image {
  width: 200rpx;
  height: 200rpx;
  margin-right: 25rpx;
}

.list_cons .right {
  width: 395rpx;
  height: 200rpx;
}

.list_cons .right .name {
  font-size: 32rpx;
  color: #333333;
  font-weight: 700;
  letter-spacing: 3rpx;
  margin-bottom: 20rpx;
}

.list_cons .right .list1 {
  margin-bottom: 17rpx;
}

.list_cons .right .list1 .condition1 {
  font-size: 28rpx;
  color: #999999;
  margin-bottom: 15rpx;
}

.list_cons .right .list1 .condition2 {
  font-size: 24rpx;
  color: #999999;
}

.list_cons .right .list1 .act {
  color: #2d8622;
}

.list_cons .right .list1 .r {
  font-size: 24rpx;
  color: #ff4965;
}

.list_cons .right .list1 .big {
  font-size: 32rpx;
}

.list_cons .right .lists2 {
  height: 36rpx;
  line-height: 36rpx;
  font-size: 22rpx;
}

.list_cons .right .lists2 .left {
  width: 162rpx;
  text-align: center;
  color: #ff4965;
  border: 1rpx solid #ff4965;
  box-sizing: border-box;
  border-radius: 5rpx;
}

.list_cons .right .lists2 .right2 {
  color: #999999;
}

.flex_center {
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
}

.flex_between {
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
}

.flex_start {
  display: flex;
  display: -webkit-flex;
  justify-content: flex-start;
  align-items: center;
}

.flex_end {
  display: flex;
  display: -webkit-flex;
  justify-content: flex-end;
  align-items: center;
}

.print-choose {
  display: flex;
  align-items: center;
}

.print-choose .top_icon {
  margin-right: 20rpx;
}

.nothing {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 250rpx;
}

.nothing image {
  width: 30%;
  margin-bottom: 40rpx;
}

.nothing button {
  width: 40%;
  color: #333;
  background: #ffe27a;
  border-radius: 60rpx;
  margin-top: 40rpx;
}