/* pages/error_book/submit_success.wxss */
page {
    width: 100%;
    height: 100%;
     font-size: 36rpx;
}

.content {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 200rpx;
}


.success {
    color: #333333;
    margin-top: 50rpx;
}

.bottom {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    border-top: 2rpx solid #CECECE;
}

.bottom>view{
    width:50%;
    height:100%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.bottom image{
    width:50rpx;
    margin-right:12rpx;
}
.left{
    color:#666;
}
.right{
    background: #FFE27A;
}