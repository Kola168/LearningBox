/* pages/package_feature/kousuan/createplan.wxss */
page {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  padding: 30rpx 24rpx;
}

view {
  box-sizing: border-box;
}

.plan-top-tip {
  width: 100%;
  font-size: 24rpx;
  color: #FFA200;
}

.check-contain-box {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 40rpx;
}

.check-name-box {
  width: 120rpx;
  font-size: 28rpx;
  color: #666666;
  margin-right: 20rpx;
  display:flex;
  justify-content:flex-end;
}

.check-name-box-space{
  width: 86rpx;
  font-size: 28rpx;
  color: #666666;
  text-align: justify;
  text-align-last: justify;
}

.check-content-box {
  width:550rpx;
  height:80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border: 2rpx solid #E7E7E7;
  border-radius: 12rpx;
  position:relative;
}

.check-value-box {
  font-size: 14px;
  color: #333333;
  margin: 20rpx;
}

.check-point-img {
  width: 150rpx;
  margin-left: 106rpx;
  margin-bottom:-30rpx;
}


.conf-check-but{
  width:308rpx;
  line-height:76rpx;
  border-radius:38rpx;
  text-align:center;
  background:#FFDC5E;
  font-size: 36rpx;
  color: #454545;
  margin:0 auto;
  margin-top:52rpx;
}

.scroll-position-box{
  width:calc(100% + 4rpx);
  position:absolute;
  z-index:1;
  top:72rpx;
  left:-2rpx;
  border: 1px solid #E7E7E7;
  border-top:1px solid #fff;
  border-bottom-left-radius: 6px;
  border-bottom-right-radius: 6px;
  overflow:hidden;
}

.scroll-list-box{
  width:100%;
  padding-top:10rpx;
  max-height:308rpx;
  overflow:scroll;
  background:#fff;
}

.scroll-list{
  width:100%;
  height:72rpx;
  background:#fff;
  font-size: 28rpx;
  color: #333333;
  padding:16rpx 22rpx;
}

.scroll-list-checked{
  background: #FFF8E0;
}

@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
    url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
    url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-down:before {
  content: "\e69b";
}

.icon-down {
  transition: 0.2s;
  font-size: 32rpx;
  color: #CECECE;
  margin-right: 20rpx;
  font-weight: 600;
}

.icon-checked {
  color: #ff9702;
  transform: rotate(-180deg);
}

.icon-rotate{
  transform:rotate(-180deg);
}
