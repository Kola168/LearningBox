page {
  background: #F4F4F4;
  min-height: 100%;
}

.content-empty {
  font-size: 30rpx;
  color: #666;
  text-align: center;
  padding-top: 40rpx;
}

.option-on {
  color: #FFA200;
}

.select-list {
  position: fixed;
  left: 0;
  top: 82rpx;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .6);
  z-index: 100;
}

.list-box {
  background: #fff;
  max-height: 60%;
  padding-left: 26rpx;
  font-size: 30rpx;
  color: #666;
  overflow: scroll;
}

.list-item {
  height: 100rpx;
  line-height: 100rpx;
}

.content {
  padding: 40rpx 24rpx 0;
}

.all-check {
  color: #FFA200;
  font-size: 30rpx;
  padding: 0 0 20rpx 30rpx;
}

.content-item {
  border-radius: 12rpx;
  background: #fff;
  margin-bottom: 30rpx;
}

.item-title {
  height: 82rpx;
  border-bottom: 1px solid #CECECE;
  justify-content: space-between;
  padding: 0 30rpx;
}

.title-l {
  font-weight: bold;
  color: #454545;
  font-size: 30rpx;
}

.title-l image {
  width: 34rpx;
  height: 34rpx;
  margin-right: 20rpx;
}

.title-r {
  color: #999;
  font-size: 24rpx;
}

.unable {
  background: #eaeaea !important;
  color: #a5a5a5 !important;
}

.print {
  background: #FFE27A;
  color: #333;
  height: 100rpx;
  line-height: 100rpx;
  font-weight: bold;
  text-align: center;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
}