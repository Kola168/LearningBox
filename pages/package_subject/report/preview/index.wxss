.swpier-box {
  width: 96%;
  height: 90%;
  margin: 0 auto;
}

.img-item {
  width: 100%;
}

.report-btns {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  display: flex;
  height: 100rpx;
  line-height: 98rpx;
}

.report-btns>view {
  width: 50%;
  font-size: 36rpx;
  flex-grow: 1;
}

.more-report {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #666;
  border-top: 2rpx solid #E7E7E7;
}

.more-report image {
  width: 36rpx;
  margin-right: 10rpx;
}

.print-report {
  color: #333;
  text-align: center;
  background: #FFDC5E;
  border-top: 2rpx solid #FFDC5E;
}