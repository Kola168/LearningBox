<nav-bar background="#fff" title="试卷详情" />
<view class="preview-main">
  <swiper class="swpier-box" bindchange="changeImg">
    <swiper-item class="" item-id="" wx:for="{{imgList}}" wx:key="*this">
      <image class="img-item" src="{{item}}" mode="widthFix"></image>
    </swiper-item>
  </swiper>
  <view class="precent">{{currentIndex}}/{{imgList.length}}</view>
  <view class="btn-bottom">
    <view class="print-answer">
      <image class="check-on" src="../../images/check_on.png"></image>
      <view class="title">打印答案</view>
    </view>
    <botttom-button class="btn-bottom-confirm" text-right="确认打印" bindrighttap="confirm" get-phone="{{true}}" />
  </view>
</view>
<confirm-print bindconfirm="print" modal="{{confirmModal}}" />
<import src="/lib/toast/wetoast.wxml" />
<template is="wetoast" data="{{...__wetoast__}}" />