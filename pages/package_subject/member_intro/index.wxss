.container {
  display: flex;
  flex-direction: column;
  position: relative;
  background: #FFDC5E;
  padding-bottom: 120rpx;
}

.container image {
  width: 100%;
}

image.free_7 {
  position: absolute;
  width: calc(100% - 24rpx);
  left: 12rpx;
  bottom: 160rpx;
}

.buy-member {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  font-size: 36rpx;
  text-align: center;
  color: #fff;
  background: linear-gradient(-90deg, #FCA33A 0%, #FB740C 100%);
}