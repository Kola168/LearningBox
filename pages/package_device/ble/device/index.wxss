.container {
    padding: 44rpx 24rpx 24rpx 24rpx;
    font-size: 36rpx;
    color: #333;
    flex-direction: column;
    align-items: center;
    width: 100%;
    height: 100%;
}

.title {
    font-size: 36rpx;
    color: #666666;
    text-align: center;
    margin: 40rpx 0 88rpx;
}

.equipment-desc {
    font-size: 30rpx;
    color: #666666;
    text-align: center;
    margin: 40rpx 0 88rpx;
}

button {
    outline: none;
    border: none;
    margin-bottom: 30rpx;
    color: #454545;
    width: 70%;
    border-radius: 100rpx;
    line-height: 2.8;
}

button.search-equipment {
    background-color: #ffe27a;
}

.xbh-example {
    width: 576rpx;
    height: 320rpx;
    margin-bottom: 200rpx;
}