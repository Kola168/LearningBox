Page {
    width: 100%;
    height: 100%;
    overflow-x: hidden;
}

.container {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.title {
    color: #333333;
    font-size: 36rpx;
    margin-top: 30rpx;
}

.wifi-list {
    width: 100%;
    margin-top: 20rpx;
}

.wifi-item {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding: 16rpx 20rpx;
    margin: 0 20rpx;
    border-bottom: #f5f5f5 solid 2rpx;
}

.wifi-item text {
    color: #333333;
    font-size: 36rpx;
}