.container {
  display: flex;
  flex-direction: column;
  padding-bottom: 40rpx;
}

.container image {
  width: 100%;
}

.print-btn {
  width: 80%;
  margin: 0 auto 20rpx;
  background: #FFDC5E;
  color: #454545;
  font-size: 36rpx;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 50rpx;
  transform: translateY(-100rpx);
}