
Page{
	width: 100%;
	height:100%;
}

.container{
	display: flex;
	flex-direction: column;
	align-items: center;
}

.box-img {
	width:40%;
	margin-top:200rpx;
}

.serial{
	margin-top:42rpx;
	color:#333;
  font-size:30rpx;
}

.btn{
	width: 80%;
	display: flex;
	justify-content: space-between;
	margin-top:90rpx;
}

.btn-left {
	width: 43%;
	height: 100rpx;
	line-height: 100rpx;
	background: #fff;
  color:#9e9e9e;
  font-size:36rpx;
  text-align:center;
	border-radius:500rpx;
	text-align: center;
	border: 2rpx solid #cecece;
}

.btn-right {
	width: 43%;
	height: 100rpx;
	line-height: 100rpx;
	background: #ffdc5e;
  color:#333;
  font-size:36rpx;
  text-align:center;
	border-radius:500rpx;
	text-align: center;
}