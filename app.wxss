@import "./lib/toast/wetoast.wxss";
.is-ipx {
  bottom: 60rpx !important;
}

.is-ipx::after {
  position: absolute;
  left: 0;
  bottom: -60rpx;
  content: '';
  width: 100%;
  height: 60rpx;
  background: #FFF;
}

/* 文字超出单行省略号 */
.text-overflow-points {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}